{
  "name": "@stzhu/eslint-config",
  "version": "0.5.0",
  "description": "Shared config for ESLint",
  "keywords": [
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevezhu/eslint-config.git"
