    "./react": "./src/react.js",
    "./node": "./src/node.js",
    "./expo": "./src/expo.js",
    "./storybook": "./src/storybook.js",
    "./vitest": "./src/vitest.js",
    "./tailwind": "./src/tailwind.js",
    "./lingui": "./src/lingui.js",
    "./turbo": "./src/turbo.js"
  },
  "scripts": {
    "format": "prettier -w .",
    "lint": "prettier -c . && eslint .",
    "test": "tsc --noEmit -p jsconfig.json"
  },
  "dependencies": {
    "@eslint/js": "^9.28.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/eslint-plugin": "^1.2.2",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-turbo": "^2.5.4",
    "eslint-plugin-better-tailwindcss": "^3.7.5",
    "eslint-plugin-expo": "^0.1.4",
    "eslint-plugin-import-x": "^4.15.2",
    "eslint-plugin-lingui": "^0.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.0.0",
    "eslint-plugin-turbo": "^2.5.4",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.34.0"
