{
  "name": "@stzhu/eslint-config",
  "version": "0.6.4",
  "description": "Shared config for ESLint",
  "keywords": [
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevezhu/eslint-config.git"
  },
  "license": "MIT",
  "author": "Steve Zhu <4130171+stevezhu@users.noreply.github.com>",
  "type": "module",
  "exports": {
    "./ts": {
      "import": {
        "types": "./dist/ts.d.ts",
        "default": "./dist/ts.js"
      },
      "require": {
        "types": "./dist/ts.d.cts",
        "default": "./dist/ts.cjs"
      }
    },
    "./react": {
      "import": {
        "types": "./dist/react.d.ts",
        "default": "./dist/react.js"
      },
      "require": {
        "types": "./dist/react.d.cts",
        "default": "./dist/react.cjs"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/node.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./dist/node.d.cts",
        "default": "./dist/node.cjs"
      }
    },
    "./expo": {
      "import": {
        "types": "./dist/expo.d.ts",
        "default": "./dist/expo.js"
      },
      "require": {
        "types": "./dist/expo.d.cts",
        "default": "./dist/expo.cjs"
      }
    },
    "./storybook": {
      "import": {
        "types": "./dist/storybook.d.ts",
        "default": "./dist/storybook.js"
      },
      "require": {
        "types": "./dist/storybook.d.cts",
        "default": "./dist/storybook.cjs"
      }
    },
    "./vitest": {
      "import": {
        "types": "./dist/vitest.d.ts",
        "default": "./dist/vitest.js"
      },
      "require": {
        "types": "./dist/vitest.d.cts",
        "default": "./dist/vitest.cjs"
      }
    },
    "./tailwind": {
      "import": {
        "types": "./dist/tailwind.d.ts",
        "default": "./dist/tailwind.js"
      },
      "require": {
        "types": "./dist/tailwind.d.cts",
        "default": "./dist/tailwind.cjs"
      }
    },
    "./lingui": {
      "import": {
        "types": "./dist/lingui.d.ts",
        "default": "./dist/lingui.js"
      },
      "require": {
        "types": "./dist/lingui.d.cts",
        "default": "./dist/lingui.cjs"
      }
    },
    "./turbo": {
      "import": {
        "types": "./dist/turbo.d.ts",
        "default": "./dist/turbo.js"
      },
      "require": {
        "types": "./dist/turbo.d.cts",
        "default": "./dist/turbo.cjs"
      }
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier -w .",
    "inspect": "eslint --inspect-config",
    "lint": "prettier -c . && eslint .",
    "test": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/js": "^9.28.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/eslint-plugin": "^1.2.2",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-turbo": "^2.5.4",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-better-tailwindcss": "^3.7.5",
    "eslint-plugin-expo": "^0.1.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-lingui": "^0.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.0.0",
    "eslint-plugin-turbo": "^2.5.4",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.34.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.2.0",
    "@stzhu/prettier-plugin-tsconfig": "^0.6.0",
    "@stzhu/tsconfig": "^0.1.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "eslint": "^9",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=20"
  }
}
